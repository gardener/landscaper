{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$ref": "#/definitions/ProviderConfiguration",
  "definitions": {
    "ArchiveAccess": {
      "properties": {
        "raw": {
          "type": "string"
        },
        "remote": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "$ref": "#/definitions/RemoteArchiveAccess"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "Chart": {
      "properties": {
        "ref": {
          "type": "string"
        },
        "fromResource": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "$ref": "#/definitions/RemoteBlueprintReference"
        },
        "archive": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "$ref": "#/definitions/ArchiveAccess"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "ExportFromManifestItem": {
      "required": [
        "key",
        "jsonPath"
      ],
      "properties": {
        "key": {
          "type": "string"
        },
        "jsonPath": {
          "type": "string"
        },
        "fromResource": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "$ref": "#/definitions/TypedObjectReference"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "ObjectReference": {
      "required": [
        "name",
        "namespace"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "namespace": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "ProviderConfiguration": {
      "required": [
        "kubeconfig",
        "chart",
        "name",
        "namespace"
      ],
      "properties": {
        "kind": {
          "type": "string"
        },
        "apiVersion": {
          "type": "string"
        },
        "kubeconfig": {
          "type": "string"
        },
        "updateStrategy": {
          "type": "string"
        },
        "chart": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "$ref": "#/definitions/Chart"
        },
        "name": {
          "type": "string"
        },
        "namespace": {
          "type": "string"
        },
        "values": {
          "items": {
            "type": "integer"
          },
          "type": "array"
        },
        "exportsFromManifests": {
          "items": {
            "$schema": "http://json-schema.org/draft-04/schema#",
            "$ref": "#/definitions/ExportFromManifestItem"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "RemoteArchiveAccess": {
      "properties": {
        "url": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "RemoteBlueprintReference": {
      "required": [
        "VersionedResourceReference"
      ],
      "properties": {
        "VersionedResourceReference": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "$ref": "#/definitions/VersionedResourceReference"
        },
        "repositoryContext": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "$ref": "#/definitions/RepositoryContext"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "RepositoryContext": {
      "required": [
        "type",
        "baseUrl"
      ],
      "properties": {
        "type": {
          "type": "string"
        },
        "baseUrl": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "ResourceReference": {
      "required": [
        "componentName",
        "resourceName"
      ],
      "properties": {
        "componentName": {
          "type": "string"
        },
        "resourceName": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "TypedObjectReference": {
      "required": [
        "apiVersion",
        "kind",
        "ObjectReference"
      ],
      "properties": {
        "apiVersion": {
          "type": "string"
        },
        "kind": {
          "type": "string"
        },
        "ObjectReference": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "$ref": "#/definitions/ObjectReference"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "VersionedResourceReference": {
      "required": [
        "ResourceReference",
        "version"
      ],
      "properties": {
        "ResourceReference": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "$ref": "#/definitions/ResourceReference"
        },
        "version": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    }
  }
}